<Template author="Matt Ward" version="1.0">	
	<Config
		name="Text Template"
		icon="TextTemplate.File"
		category="C#"
		defaultname="TextTemplate${Number}.txt"
		language="Default"/>
	 
	<Description>A t4 text template for the C# language.</Description>

	<Files>
		<File name="${FullName}" language="Default"><![CDATA[<#@ template language="C#" #>
<#@ output extension=".txt" #>
]]></File>
	</Files>
	<AdditionalOptions/>
</Template>
